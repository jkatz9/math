, 
\begin_inset Formula 
\begin{eqnarray*}
b_{12}(\vec{x}_{1},\vec{x}_{2}) & = & \det\begin{bmatrix}e_{12} & \vec{x}_{1} & \vec{x}_{2}\end{bmatrix}=0\\
b_{23}(\vec{x}_{2},\vec{x}_{3}) & = & \det\begin{bmatrix}e_{23} & \vec{x}_{2} & \vec{x}_{3}\end{bmatrix}=0\\
b_{13}(\vec{x}_{1},\vec{x}_{3}) & = & \det\begin{bmatrix}e_{13} & \vec{x}_{1} & \vec{x}_{3}\end{bmatrix}=0
\end{eqnarray*}

