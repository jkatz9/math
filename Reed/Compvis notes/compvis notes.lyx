\begin_layout Itemize
It is a general fact that three noncolinear points define a unique plane
 in projective space.
 If the three distinct points are 
\begin_inset Formula $X_{1},X_{2},X_{3}$
\end_inset

, then the generated plane is given by 
\begin_inset Formula 
\[
\pi=\begin{pmatrix}(X_{1}-X_{3})\times(X_{2}-X_{3})\\
-X_{3}^{\top}(X_{1}\times X_{2})
\end{pmatrix},
\]

\end_inset

 a length four vector who's null space is the plane defined by the tree
 points.
 
\end_layout

\begin_deeper
\begin_layout Itemize
We can use this to determine the trifocal planes:
\begin_inset Formula 
\begin{eqnarray*}
T_{1,2,3} & = & \begin{pmatrix}a_{2}a_{6}-a_{3}a_{5}\\
a_{3}a_{4}-a_{1}a_{6}\\
a_{1}a_{5}-a_{2}a_{4}\\
0
\end{pmatrix}\\
T_{1,3,4} & = & \begin{pmatrix}a_{5}a_{9}-a_{6}a_{8}\\
a_{6}a_{7}-a_{4}a_{9}\\
a_{4}a_{8}-a_{5}a_{7}\\
0
\end{pmatrix}\\
T_{2,3,4} & = & \begin{pmatrix}-a_{3}a_{5}+a_{2}a_{6}+a_{3}a_{8}-a_{6}a_{8}-a_{2}a_{9}+a_{5}a_{9}\\
a_{3}a_{4}-a_{1}a_{6}-a_{3}a_{7}+a_{6}a_{7}+a_{1}a_{9}-a_{4}a_{9},\\
-a_{2}a_{4}+a_{1}a_{5}+a_{2}a_{7}-a_{5}a_{7}-a_{1}a_{8}+a_{4}a_{8}\\
(-a_{3}a_{5}+a_{2}a_{6})a_{7}+(a_{3}a_{4}-a_{1}a_{6})a_{8}+(-a_{2}a_{4}+a_{1}a_{5})a_{9}
\end{pmatrix}.
\end{eqnarray*}

\end_inset


\end_layout

\end_deeper
\begin_layout Itemize
A general formula for 
\begin_inset Formula $b_{i,j}$
\end_inset

, obtained by subtracting each row of the first camera from the corresponding
 row of the second camera, and expanding along the diagonal identity in
 the first.
 
\begin_inset Formula 
\begin{eqnarray*}
b_{i,j}(x_{i},y_{i},z_{i},x_{j},y_{j},z_{j}) & = & \begin{vmatrix}a_{i}-a_{j} & -x_{j} & x_{i}\\
b_{i}-b_{j} & -y_{j} & y_{i}\\
c_{i}-c_{j} & -z_{j} & z_{i}
\end{vmatrix}\\
 & = & (a_{i}-a_{j})\begin{vmatrix}y_{j} & y_{i}\\
z_{j} & z_{i}
\end{vmatrix}-\left(b_{i}-b_{j}\right)\begin{vmatrix}x_{j} & x_{i}\\
z_{j} & z_{i}
\end{vmatrix}+\left(c_{i}-c_{j}\right)\begin{vmatrix}x_{j} & x_{i}\\
y_{j} & y_{i}
\end{vmatrix}
\end{eqnarray*}

\end_inset

 
\end_layout

\begin_layout Itemize
In the case of 
\begin_inset Formula $\tilde{I}_{b}$
\end_inset

, we will be looking at 
\begin_inset Formula 
\begin{eqnarray*}
b_{i,i+1} & = & (a_{i}-a_{i+1})\begin{vmatrix}y_{i+1} & y_{i}\\
z_{i+1} & z_{i}
\end{vmatrix}-\left(b_{i}-b_{i+1}\right)\begin{vmatrix}x_{i+1} & x_{i}\\
z_{i+1} & z_{i}
\end{vmatrix}+\left(c_{i}-c_{i+1}\right)\begin{vmatrix}x_{i+1} & x_{i}\\
y_{i+1} & y_{i}
\end{vmatrix}\\
 & = & (a_{i}-a_{i+1})M_{i,i+1}^{\sim x}-\left(b_{i}-b_{i+1}\right)M_{i,i+1}^{\sim y}+\left(c_{i}-c_{i+1}\right)M_{i,i+1}^{\sim z}
\end{eqnarray*}

\end_inset

and 
\begin_inset Formula 
\begin{eqnarray*}
b_{i,i+2} & = & (a_{i}-a_{i+2})\begin{vmatrix}y_{i+2} & y_{i}\\
z_{i+2} & z_{i}
\end{vmatrix}-\left(b_{i}-b_{i+2}\right)\begin{vmatrix}x_{i+2} & x_{i}\\
z_{i+2} & z_{i}
\end{vmatrix}+\left(c_{i}-c_{i+2}\right)\begin{vmatrix}x_{i+2} & x_{i}\\
y_{i+2} & y_{i}
\end{vmatrix}\\
 & = & (a_{i}-a_{i+2})M_{i,i+2}^{\sim x}-\left(b_{i}-b_{i+2}\right)M_{i,i+2}^{\sim y}+\left(c_{i}-c_{i+2}\right)M_{i,i+2}^{\sim z}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
Then 
\begin_inset Formula 
\begin{eqnarray*}
\tilde{I}_{b} & = & (b_{i,i+1},b_{i,i+2})\\
 & = & \{\sum_{i=1}^{m-1}r_{i}\left((a_{i}-a_{i+1})\begin{vmatrix}y_{i+1} & y_{i}\\
z_{i+1} & z_{i}
\end{vmatrix}-\left(b_{i}-b_{i+1}\right)\begin{vmatrix}x_{i+1} & x_{i}\\
z_{i+1} & z_{i}
\end{vmatrix}+\left(c_{i}-c_{i+1}\right)\begin{vmatrix}x_{i+1} & x_{i}\\
y_{i+1} & y_{i}
\end{vmatrix}\right)+\sum_{i=1}^{m-2}t_{i}\left((a_{i}-a_{i+2})\begin{vmatrix}y_{i+2} & y_{i}\\
z_{i+2} & z_{i}
\end{vmatrix}-\left(b_{i}-b_{i+2}\right)\begin{vmatrix}x_{i+2} & x_{i}\\
z_{i+2} & z_{i}
\end{vmatrix}+\left(c_{i}-c_{i+2}\right)\begin{vmatrix}x_{i+2} & x_{i}\\
y_{i+2} & y_{i}
\end{vmatrix}\right):r_{i},t_{i}\in R\}\\
 & = & \{\sum_{i=1}^{m-1}\left(\left(r_{i}(a_{i}-a_{i+1})+t_{i}(a_{i}-a_{i+2})\right)\begin{vmatrix}y_{i+1}+y_{i+2} & y_{i}\\
z_{i+1}+z_{i+2} & z_{i}
\end{vmatrix}-\left(r_{i}\left(b_{i}-b_{i+1}\right)+t_{i}\left(b_{i}-b_{i+2}\right)\right)\begin{vmatrix}x_{i+1}+x_{i+2} & x_{i}\\
z_{i+1}+z_{i+2} & z_{i}
\end{vmatrix}+\left(r_{i}\left(c_{i}-c_{i+1}\right)+t_{i}\left(c_{i}-c_{i+2}\right)\right)\begin{vmatrix}x_{i+1}+x_{i+2} & x_{i}\\
y_{i+1}+y_{i+2} & y_{i}
\end{vmatrix}\right)r_{i},t_{i}\in R\}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
A general form for 
\begin_inset Formula $T_{ijk}$
\end_inset

 can be derived as follows.
 Take cameras 
\begin_inset Formula $k,i,j$
\end_inset

, and keep rows 
\begin_inset Formula $p_{i},q_{i}\in\{1,2,3\}$
\end_inset

 from camera 
\begin_inset Formula $i$
\end_inset

, and rows 
\begin_inset Formula $p_{j},q_{j}\in\{1,2,3\}$
\end_inset

 from camera 
\begin_inset Formula $j$
\end_inset

.
 We then look at the determinant
\begin_inset Formula 
\[
\]

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
T_{ki^{p_{i}q_{i}}j^{p_{j}q_{j}}} & = & \begin{vmatrix}1 & 0 & 0 & a_{k} & x_{k} & 0 & 0\\
0 & 1 & 0 & b_{k} & y_{k} & 0 & 0\\
0 & 0 & 1 & c_{k} & z_{k} & 0 & 0\\
0 & 0 & 0 & \vec{a}_{i}^{p_{i}}-\vec{a}_{k}^{p_{i}} & -\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}} & 0\\
0 & 0 & 0 & \vec{a}_{i}^{q_{i}}-\vec{a}_{k}^{q_{i}} & -\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}} & 0\\
0 & 0 & 0 & \vec{a}_{j}^{p_{j}}-\vec{a}_{k}^{p_{j}} & -\vec{x}_{k}^{p_{j}} & 0 & \vec{x}_{j}^{p_{j}}\\
0 & 0 & 0 & \vec{a}_{j}^{q_{j}}-\vec{a}_{k}^{q_{j}} & -\vec{x}_{k}^{q_{j}} & 0 & \vec{x}_{j}^{q_{j}}
\end{vmatrix}\\
 & = & \begin{vmatrix}\vec{a}_{i}^{p_{i}}-\vec{a}_{k}^{p_{i}} & -\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}} & 0\\
\vec{a}_{i}^{q_{i}}-\vec{a}_{k}^{q_{i}} & -\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}} & 0\\
\vec{a}_{j}^{p_{j}}-\vec{a}_{k}^{p_{j}} & -\vec{x}_{k}^{p_{j}} & 0 & \vec{x}_{j}^{p_{j}}\\
\vec{a}_{j}^{q_{j}}-\vec{a}_{k}^{q_{j}} & -\vec{x}_{k}^{q_{j}} & 0 & \vec{x}_{j}^{q_{j}}
\end{vmatrix}\\
 & = & (\vec{a}_{i}^{p_{i}}-\vec{a}_{k}^{p_{i}})\begin{vmatrix}\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}} & 0\\
\vec{x}_{k}^{p_{j}} & 0 & \vec{x}_{j}^{p_{j}}\\
\vec{x}_{k}^{q_{j}} & 0 & \vec{x}_{j}^{q_{j}}
\end{vmatrix}-(\vec{a}_{i}^{q_{i}}-\vec{a}_{k}^{q_{i}})\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}} & 0\\
\vec{x}_{k}^{p_{j}} & 0 & \vec{x}_{j}^{p_{j}}\\
\vec{x}_{k}^{q_{j}} & 0 & \vec{x}_{j}^{q_{j}}
\end{vmatrix}\\
 &  & +(\vec{a}_{j}^{p_{j}}-\vec{a}_{k}^{p_{j}})\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}} & 0\\
\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}} & 0\\
\vec{x}_{k}^{q_{j}} & 0 & \vec{x}_{j}^{q_{j}}
\end{vmatrix}-(\vec{a}_{j}^{q_{j}}-\vec{a}_{k}^{q_{j}})\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}} & 0\\
\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}} & 0\\
\vec{x}_{k}^{p_{j}} & 0 & \vec{x}_{j}^{p_{j}}
\end{vmatrix}\\
 & = & -(\vec{a}_{i}^{p_{i}}-\vec{a}_{k}^{p_{i}})\vec{x}_{i}^{q_{i}}\begin{vmatrix}\vec{x}_{k}^{p_{j}} & \vec{x}_{j}^{p_{j}}\\
\vec{x}_{k}^{q_{j}} & \vec{x}_{j}^{q_{j}}
\end{vmatrix}+(\vec{a}_{i}^{q_{i}}-\vec{a}_{k}^{q_{i}})\vec{x}_{i}^{p_{i}}\begin{vmatrix}\vec{x}_{k}^{p_{j}} & \vec{x}_{j}^{p_{j}}\\
\vec{x}_{k}^{q_{j}} & \vec{x}_{j}^{q_{j}}
\end{vmatrix}\\
 &  & +(\vec{a}_{j}^{p_{j}}-\vec{a}_{k}^{p_{j}})\vec{x}_{j}^{q_{j}}\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}}\\
\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}}
\end{vmatrix}-(\vec{a}_{j}^{q_{j}}-\vec{a}_{k}^{q_{j}})\vec{x}_{j}^{p_{j}}\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}}\\
\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}}
\end{vmatrix}\\
 & = & \left(-(\vec{a}_{i}^{p_{i}}-\vec{a}_{k}^{p_{i}})\vec{x}_{i}^{q_{i}}+(\vec{a}_{i}^{q_{i}}-\vec{a}_{k}^{q_{i}})\vec{x}_{i}^{p_{i}}\right)\begin{vmatrix}\vec{x}_{k}^{p_{j}} & \vec{x}_{j}^{p_{j}}\\
\vec{x}_{k}^{q_{j}} & \vec{x}_{j}^{q_{j}}
\end{vmatrix}+\left((\vec{a}_{j}^{p_{j}}-\vec{a}_{k}^{p_{j}})\vec{x}_{j}^{q_{j}}-(\vec{a}_{j}^{q_{j}}-\vec{a}_{k}^{q_{j}})\vec{x}_{j}^{p_{j}}\right)\begin{vmatrix}\vec{x}_{k}^{p_{i}} & \vec{x}_{i}^{p_{i}}\\
\vec{x}_{k}^{q_{i}} & \vec{x}_{i}^{q_{i}}
\end{vmatrix}\\
 & = & -\vec{a}_{i}^{p_{i}}\vec{x}_{i}^{q_{i}}\vec{x}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}+\vec{a}_{i}^{p_{i}}\vec{x}_{i}^{q_{i}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{q_{j}}+\vec{a}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}\vec{x}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}-\vec{a}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{q_{j}}\\
 &  & +\vec{a}_{i}^{q_{i}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}-\vec{a}_{i}^{q_{i}}\vec{x}_{i}^{p_{i}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{q_{j}}-\vec{a}_{k}^{q_{i}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}+\vec{a}_{k}^{q_{i}}\vec{x}_{i}^{p_{i}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{q_{j}}\\
 &  & +\vec{a}_{j}^{p_{j}}\vec{x}_{j}^{q_{j}}\vec{x}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}-\vec{a}_{j}^{p_{j}}\vec{x}_{j}^{q_{j}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{q_{i}}-\vec{a}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}\vec{x}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}+\vec{a}_{k}^{p_{j}}\vec{x}_{j}^{q_{j}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{q_{i}}\\
 &  & -\vec{a}_{j}^{q_{j}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}+\vec{a}_{j}^{q_{j}}\vec{x}_{j}^{p_{j}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{q_{i}}+\vec{a}_{k}^{q_{j}}\vec{x}_{j}^{p_{j}}\vec{x}_{k}^{p_{i}}\vec{x}_{i}^{q_{i}}-\vec{a}_{k}^{q_{j}}\vec{x}_{j}^{p_{j}}\vec{x}_{i}^{p_{i}}\vec{x}_{k}^{q_{i}}\\
 & = & T_{ki^{p_{i},q_{i}}j^{p_{j},q_{j}}}
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
(HERE!)Notice that any choice of 
\begin_inset Formula $p_{i},q_{i}$
\end_inset

, 
\begin_inset Formula $p_{j},q_{j}\in\left[3\right]$
\end_inset

 will have at least one overlap.
 Wlog set 
\begin_inset Formula $p_{i}=p_{j}=p$
